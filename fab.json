---
layout: null
---



[
  {% for ep in site.posts %}
  {
    "id": "{{ ep.url | remove: ".html" | split: "/" | last }}",
    "title": "{{ ep.title }}",
    "description": "{{ ep.summary }}",
    "duration": "{{ ep.podcast_duration }}",
    "thumbURL": "{{site.url}}/{{ ep.mp4_thumbnail | escape }}",
    "imgURL": "{{site.url}}/{{ ep.mp4_thumbnail | escape }}",
    "videoURL": "{{ ep.mp4_url}}",
    "categories": [
      "{{ ep.meetup }}"
    ],
    "channel_id": "{{ ep.categories }}",
    "recommendations": [
      {% for tag in ep.tags %}
      {% for mypost in site.tags.[tag] %}
      "{{ mypost.url | remove: ".html" | split: "/" | last | }}" {% unless forloop.last %},{% endunless %}
      {% endfor %}{% unless forloop.last %},{% endunless %}
      {% endfor %}
    ]
    ,
    "maturityRating": "PG",
    "live": "1",
    "startTime": 1490393748,
    "endTime": 1490397347,
    "videoPreviewUrl": "http://mywebsite.com/some/url/to/the/video.mp4",
    "imdbId": "tt2417148",
    "closeCaptionUrls": "1",
    "genres": "DRAMA"

  } {% unless forloop.last %},{% endunless %}
{% endfor %},
{
"globalRecommendations": [
  {% assign globalRecs = site.posts | where: "global_recommendation", true %}
{% for ep in globalRecs %}
  "{{ ep.url | remove: ".html" | split: "/" | last }}"{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
}
]
